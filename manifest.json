{
    "manifest_version": 3,
    "name": "CodeSprint Timer",
    "version": "1.0",
    "description": "A timer for practicing coding questions.",
    "permissions": ["storage", "notifications"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "48": "icon.png"
    }
  }
  